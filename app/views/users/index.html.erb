<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
   
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        
        <td><%= link_to 'Show', user %></td>
        <div>
        <ul>
        <% @email.each do |email| %>

<%= email.email %>

<% end %>
        
      </tr>
    <% end %>
  </tbody>
</table>


